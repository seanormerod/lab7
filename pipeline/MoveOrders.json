{
	"name": "MoveOrders",
	"properties": {
		"activities": [
			{
				"name": "MoveOrdersFromSourceToPool",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"wildcardFileName": "*.json",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "SqlPoolSink",
						"preCopyScript": "TRUNCATE table orders",
						"writeBehavior": "Insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['id']"
								},
								"sink": {
									"name": "id"
								}
							},
							{
								"source": {
									"path": "$['date']"
								},
								"sink": {
									"name": "date"
								}
							},
							{
								"source": {
									"path": "$['value']"
								},
								"sink": {
									"name": "value"
								}
							},
							{
								"source": {
									"path": "$['customer_id']"
								},
								"sink": {
									"name": "customer_id"
								}
							},
							{
								"source": {
									"path": "$['product_id']"
								},
								"sink": {
									"name": "product_id"
								}
							}
						],
						"mapComplexValuesToString": false
					}
				},
				"inputs": [
					{
						"referenceName": "OrderTable",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Orders_DedicatedPool",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}